<grammar>
    <BNFdef name="delete statement" predicative="no">
        <rhs>
            <opt>
                <alt>
                    <kw>DETACH</kw>
                </alt>
                <alt>
                    <kw>NODETACH</kw>
                </alt>
            </opt>
            <kw>DELETE</kw>
            <BNF name="delete item list"/>
        </rhs>
    </BNFdef>
</grammar>